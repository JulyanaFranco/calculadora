          <TableColumn prefWidth="113.0" text="Primeiro Número">
              <cellValueFactory>
                  <PropertyValueFactory property="primeiroValor" />
              </cellValueFactory>
            </TableColumn>
          <TableColumn minWidth="3.0" prefWidth="118.0" text="Segundo Número">
              <cellValueFactory>
                  <PropertyValueFactory property="segundoValor" />
              </cellValueFactory>
              </TableColumn>
            <TableColumn prefWidth="90.0" text="Operador">
                <cellValueFactory>
                  <PropertyValueFactory property="operador" />
              </cellValueFactory>
              </TableColumn>
            <TableColumn prefWidth="120.0" text="Resultado">
                <cellValueFactory>
                  <PropertyValueFactory property="resultado" />
              </cellValueFactory>
              </TableColumn>
        </columns>
      </TableView>
      <Button layoutX="250.0" layoutY="303.0" mnemonicParsing="false" onAction="#Fechar" text="Fechar" />
      <Button layoutX="14.0" layoutY="34.0" mnemonicParsing="false" onAction="#calculadora" prefHeight="33.0" prefWidth="135.0" text="Voltar a Calculadora" />
   </children>
</AnchorPane>
